{
  "manifest_version": 2,
  "name": "SOPHOS Relay",
  "version": "1.1",
  "permissions": ["webRequest", "webRequestBlocking", "storage", "activeTab", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "SOPHOS Relay"
  },
  "options_page": "settings.html"
}
